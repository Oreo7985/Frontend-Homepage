{
  "name": "frontend-homepage",
  "homepage": "https://github.com/Oreo7985/Frontend-Homepage",
  "private": true,
  "engine": {
    "node": ">=18.18.0"
  },
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "NODE_ENV=production vite build",
    "postbuild": "cp public/_headers dist/ && cp public/_routes.json dist/ && cp public/_redirects dist/",
    "postinstall": "npm run build:prepare",
    "build:prepare": "node ./scripts/prepare-build.js",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prebuild": "rm -rf node_modules/.vite"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "axios": "^1.7.7",
    "gh-pages": "^6.2.0",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.27.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "latest",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "globals": "^15.11.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "vite": "latest"
  }
}
